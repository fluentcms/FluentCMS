<PluginContent Plugin="@Plugin">
    <ChildContent>
        @foreach (var item in Content ?? [])
        {
            <PluginItemContainer Plugin="@Plugin" Id="@item.Id">
                @((MarkupString)item.Content)
                <PluginItemActions ItemTypeName="TextHTMLContent" EditName="Edit TextHTML" />
            </PluginItemContainer>
        }
    </ChildContent>
    <ActionsEnd>
        <div data-plugin-item-action="Edit TextHTML">
            <svg class="f-action-icon" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 12h14m-7 7V5"/>
            </svg>
        </div>
    </ActionsEnd>
</PluginContent>


