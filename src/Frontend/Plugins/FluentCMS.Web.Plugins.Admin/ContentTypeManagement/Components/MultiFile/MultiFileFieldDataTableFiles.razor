@inherits BaseFieldValueComponent<ICollection<Guid>, MultiFileFieldModel>
@namespace FluentCMS.Web.Plugins.Admin.ContentTypeManagement

<Stack>
    @foreach(var file in FieldValue.Value)
    {
        @if (!string.IsNullOrEmpty(GetFileName(file)))
        {
            <Button Link Href="@GetDownloadUrl(file)">@GetFileName(file)</Button>
        }
    }
</Stack>
