@inherits BasePlugin
@rendermode InteractiveServer

<PluginForm Model="@Model" Name="@FORM_NAME" Title="Global Setting" OnSubmit="OnSubmit">
    <FormInput @bind-Value="SuperAdminUserNames" Label="Super Admin User Names" Placeholder="Admin, James, David " Required />
     <FormActions>
        <Button Color="Color.Primary" Type="ButtonType.Submit">
            Submit
        </Button>
        <Button Href="/admin">
            Cancel
        </Button>
    </FormActions>
</PluginForm>
