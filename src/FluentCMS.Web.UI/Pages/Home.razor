@page "/{*Route:nonfile}"
@inherits BasePage

<PageTitle>@AppState?.Page?.Title</PageTitle>

<HeadContent>
    @if (AppState?.Layout != null)
    {
        @((MarkupString)AppState.Layout.Head)
    }
</HeadContent>

<CascadingValue Value="@AppState">
    <Section Name="Main" />
    @* @dynamicComponent() *@
</CascadingValue>
