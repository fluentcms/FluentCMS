{
    "name": "fluentcms.web.ui",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "tailwind:watch": "postcss scss/app.css -o ../wwwroot/css/app.min.css --watch",
        "tailwind": "postcss scss/app.css -o ../wwwroot/css/app.min.css --minify",
        "sass:watch": "sass ./scss/App.scss:./scss/app.css --watch",
        "sass": "sass ./scss/App.scss:./scss/app.css",
        "watch": "conc \"npm run tailwind:watch\" \"npm run sass:watch\"",
        "build": "npm run sass && npm run tailwind"
    },
    "devDependencies": {
        "conc": "^0.0.3",
        "concurrently": "^8.2.2",
        "flowbite": "^2.1.1",
        "postcss": "^8.4.33",
        "postcss-cli": "^11.0.0",
        "postcss-selector-replace": "^1.0.2",
        "sass": "^1.69.5"
    },
    "dependencies": {
        "tailwindcss": "^3.4.1"
    }
}
