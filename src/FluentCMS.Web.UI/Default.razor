@page "/{**Route:nonfile}"

<PageTitle>@Page?.Title</PageTitle>
<HeadContent>
    @if (Page?.Layout?.Head != null)
    {
        @((MarkupString)Page.Layout.Head)
    }
</HeadContent>

<ExtendedErrorBoundary OnError="OnError">
@ChildComponents()
</ExtendedErrorBoundary>

<ConfirmProvider></ConfirmProvider>
<ToastProvider></ToastProvider>
