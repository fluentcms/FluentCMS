@inherits Input

<FormField Id=@Id Required=@Required Label=@Label>
    <Input
		Disabled=@Disabled
		Id=@Id
		Name=@Name
		Placeholder=@Placeholder
		Required=@Required
		Type=@Type
		@attributes=@AdditionalAttributes
		@bind-Value=@Value
	/>
</FormField>

@code {
    [Parameter]
    public string Label {get; set;} = "";
}
