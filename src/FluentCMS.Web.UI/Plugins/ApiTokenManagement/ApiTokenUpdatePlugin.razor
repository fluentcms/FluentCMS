@inherits BasePlugin
@rendermode InteractiveServer

<PluginForm Model="@Model" Name="@FORM_NAME" Title="Update API Token" OnSubmit="OnSubmit">
    <FormInput @bind-Value="Model!.Name" Label="Name" Placeholder="Enter Name" Required />
    <PluginFormTextarea @bind-Value="Model!.Description" Label="Description" Placeholder="Enter Description" />
    <PluginFormPolicies @bind-Value="Model!.Policies" Policies="@Policies" />
    <PluginFormActions Cancel Submit />
</PluginForm>
