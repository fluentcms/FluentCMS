@inherits BasePlugin

<PluginForm Model="@Model" Title="Change Password" OnSubmit="@OnSubmit">
    <PluginFormInput @bind-Value="@Model.OldPassword" Label="Old Password" Placeholder="Enter Old Password ..."></PluginFormInput>
    <PluginFormInput @bind-Value="@Model.NewPassword" Label="New Password" Placeholder="Enter New Password ..."></PluginFormInput>

    <PluginFormActions Submit="true" Cancel="true"> </PluginFormActions>
</PluginForm>
